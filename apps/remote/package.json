{
	"name": "remote",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "NEXT_PRIVATE_LOCAL_WEBPACK=true next dev -p 3334",
		"dev-turbo": "next dev --turbo -p 3334",
		"dev-debug": "cross-env NODE_OPTIONS='--inspect' next dev -p 3334",
		"build": "next build",
		"start": "next start -p 3334",
		"serve": "serve out -p 3334",
		"lint": "next lint",
		"lint:fix": "next lint --fix .",
		"format": "prettier -c .",
		"format:fix": "prettier --write .",
		"types:check": "tsc --noEmit",
		"clean": "rm -rf .turbo node_modules .next out .cache tsconfig.tsbuildinfo"
	},
	"dependencies": {
		"@module-federation/nextjs-mf": "8.1.0-canary.5",
		"@radix-ui/react-slot": "^1.0.2",
		"@tanstack/react-query": "5.8.1",
		"@tanstack/react-query-devtools": "5.8.1",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.0.0",
		"lucide-react": "^0.292.0",
		"next": "13.4.19",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-error-boundary": "^4.0.11",
		"tailwind-merge": "^2.0.0"
	},
	"devDependencies": {
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/node": "^20.8.10",
		"@types/react": "^18.2.36",
		"@types/react-dom": "^18.2.14",
		"autoprefixer": "^10.4.16",
		"cross-env": "^7.0.3",
		"eslint-config-custom": "workspace:*",
		"postcss": "^8.4.31",
		"postcss-import": "^15.1.0",
		"prettier-config-custom": "workspace:*",
		"serve": "^14.2.1",
		"tailwindcss": "^3.3.5",
		"tailwindcss-animate": "^1.0.7",
		"tsconfig": "workspace:*",
		"typescript": "^5.2.2"
	}
}
