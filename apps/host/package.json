{
	"name": "host",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "NEXT_PRIVATE_LOCAL_WEBPACK=true next dev",
		"dev-turbo": "next dev --turbo",
		"dev-debug": "cross-env NODE_OPTIONS='--inspect' next dev",
		"build": "next build",
		"build-standalone": "next build && mv .next/static .next/standalone/apps/host/.next",
		"start": "next start",
		"start-standalone": "node .next/standalone/apps/host/server.js",
		"lint": "next lint",
		"lint:fix": "next lint --fix .",
		"format": "prettier -c .",
		"format:fix": "prettier --write .",
		"types:check": "tsc --noEmit",
		"clean": "rm -rf .turbo node_modules .next out .cache tsconfig.tsbuildinfo"
	},
	"dependencies": {
		"@hookform/resolvers": "^3.3.2",
		"@module-federation/nextjs-mf": "8.1.0-canary.5",
		"@module-federation/utilities": "3.0.3-0",
		"@radix-ui/react-icons": "^1.3.0",
		"@radix-ui/react-slot": "^1.0.2",
		"@tanstack/react-query": "5.8.1",
		"@tanstack/react-query-devtools": "5.8.1",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.0.0",
		"fets": "^0.6.2",
		"lucide-react": "^0.292.0",
		"next": "13.4.19",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-error-boundary": "^4.0.11",
		"react-hook-form": "^7.48.2",
		"tailwind-merge": "^2.0.0",
		"zod": "^3.22.4",
		"zustand": "^4.4.6"
	},
	"devDependencies": {
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/node": "^20.8.10",
		"@types/react": "^18.2.36",
		"@types/react-dom": "^18.2.14",
		"autoprefixer": "^10.4.16",
		"cross-env": "^7.0.3",
		"eslint-config-custom": "workspace:*",
		"postcss": "^8.4.31",
		"postcss-import": "^15.1.0",
		"prettier-config-custom": "workspace:*",
		"tailwindcss": "^3.3.5",
		"tailwindcss-animate": "^1.0.7",
		"tsconfig": "workspace:*",
		"typescript": "^5.2.2"
	}
}
